<%- | 
  Type::PortNumber $port,
  Type::IPAddress $public_address,
  String[1] $serverid,
  String[1] $servername,
| -%>
<serverinfo>
<server>
<id><%= $serverid %></id>
<label><%= $servername %></label>
<hostaddress><%= $public_address %></hostaddress>
<port><%= $port %></port>
<protocol>DOP</protocol>
</server>
</serverinfo>
